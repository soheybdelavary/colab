        "def max100():\n",
        "    a=input()\n",
        "    a=int(a)\n",
        "    max=a\n",
        "    for i in rang (0,100):\n",
        "        a=int (input())\n",
        "        if a>max:\n",
        "             max=a\n",
        "        return max"
